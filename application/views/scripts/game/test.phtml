    function showRGB($rgb)
    {
        echo 'array(';
        foreach ($rgb as $v) {
            echo "'" . $v . "',";
        }
        echo '),';
    }

    function getSymbol($rgb)
    {
        if ($this->eachType($rgb, $this->water)) {
            return 'w';
        }
        if ($this->eachType($rgb, $this->smallMountain)) {
            return 'm';
        }
        if ($this->eachType($rgb, $this->bigMountain)) {
            return 'M';
        }
        if ($this->eachType($rgb, $this->forest)) {
            return 'f';
        }
        if ($this->eachType($rgb, $this->road)) {
            return 'r';
        }
        if ($this->eachType($rgb, $this->bridge)) {
            return 'b';
        }
        if ($this->eachType($rgb, $this->swamp)) {
            return 's';
        }

        return 'g';
    }

    function eachType($rgb, $type)
    {
        foreach ($type as $array) {
            if ($this->checkType($rgb, $array)) {
                return true;
            }
        }
    }

    function checkType($rgb, $type)
    {
        foreach ($rgb as $k => $v) {
            if ($v != $type[$k])
                return;
        }

        return true;
    }


    function getRGB($rgb)
    {
        $r = ($rgb >> 16) & 0xFF;
        $g = ($rgb >> 8) & 0xFF;
        $b = $rgb & 0xFF;
        return array($r, $g, $b);
    }

